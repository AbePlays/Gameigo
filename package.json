{
  "name": "gameigo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "next build",
    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc",
    "cy:run": "cypress run --config-file=cypress.config.ts",
    "cy:open": "cypress open --config-file=cypress.config.ts",
    "dev": "next dev",
    "format": "npm run prettier -- --write",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "serve": "npm run build && npm start",
    "start": "next start",
    "test:watch": "jest --watch",
    "test": "jest --coverage --runInBand",
    "validate": "npm run check-types && npm run check-format && npm run lint && npm run build"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.6.1",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@radix-ui/react-icons": "^1.3.0",
    "firebase": "^9.22.0",
    "firebase-admin": "^11.8.0",
    "formik": "^2.2.9",
    "framer-motion": "^10.12.12",
    "next": "^13.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.3",
    "@next/eslint-plugin-next": "^13.4.3",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-jest": "^29.5.0",
    "cypress": "^12.12.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "msw": "^1.2.1",
    "node-modules-regexp": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "whatwg-fetch": "^3.6.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.--write": "prettier --write"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  }
}
